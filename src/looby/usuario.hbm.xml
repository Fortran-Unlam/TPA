<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://www.hibernate.org/dtd/hibernate-configuration-3.0.dtd">
<hibernate-mapping>
	<class name="looby.Usuario" table="Usuario">
		<id column="id" name="id" type="int" />
		<property column="username" name="username" type="string" />
		<property column="password" name="password" type="string" />
		<property column="puntos" name="puntos" type="int" />
		<property column="cantidad_fruta_comida" name="cantidadFrutaComida" type="int" />
		<property column="asesinatos" name="asesinatos" type="int" />
		<property column="muertes" name="muertes" type="int" />
		<property column="partidas_ganadas" name="partidasGanadas" type="int" />
		<property column="rondas_ganadas" name="rondasGanadas" type="int" />
	</class>
</hibernate-mapping>
