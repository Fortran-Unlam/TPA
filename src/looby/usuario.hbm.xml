<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <class name="looby.Usuario" table="Usuario">
  <id name="id" type="int">
   <column name="id" not-null="true"/>
   <generator class="assigned"/>
  </id>
  <property generated="never" lazy="false" name="username" type="java.lang.String">
   <column name="username" not-null="true" sql-type="VARCHAR" unique="true"/>
  </property>
  <property generated="never" lazy="false" name="password" type="java.lang.String">
   <column name="password" not-null="true" sql-type="VARCHAR"/>
  </property>
  <property generated="never" lazy="false" name="puntos" type="int">
   <column name="puntos" not-null="true" sql-type="INTEGER"/>
  </property>
  <property generated="never" lazy="false" name="cantidadFrutaComida" type="int">
   <column name="cantidad_fruta_comida" not-null="true" sql-type="INTEGER"/>
  </property>
  <property generated="never" lazy="false" name="asesinatos" type="int">
   <column name="asesinatos" not-null="true" sql-type="INTEGER"/>
  </property>
  <property generated="never" lazy="false" name="muertes" type="int">
   <column name="muertes" not-null="true" sql-type="INTEGER"/>
  </property>
  <property generated="never" lazy="false" name="partidasGanadas" type="int">
   <column name="partidas_ganadas" not-null="true" sql-type="INTEGER"/>
  </property>
  <property generated="never" lazy="false" name="rondasGanadas" type="int">
   <column name="rondas_ganadas" not-null="true" sql-type="INTEGER"/>
  </property>
 </class>
</hibernate-mapping>
